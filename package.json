{
  "name": "openai-fetch",
  "version": "3.4.2",
  "description": "OpenAI client powered by fetch",
  "repository": "dexaai/openai-fetch",
  "license": "MIT",
  "author": {
    "name": "Riley Tomasek",
    "email": "hi@rile.yt",
    "url": "https://rile.yt"
  },
  "packageManager": "pnpm@9.12.1",
  "engines": {
    "node": ">=18"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "types": "dist/index.d.ts",
  "main": "dist/cjs/index.js",
  "sideEffects": false,
  "files": [
    "dist",
    "openai-types"
  ],
  "scripts": {
    "build": "pnpm run build:esm && pnpm run build:cjs && pnpm run copy-types",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:cjs": "tsc --project tsconfig.cjs.json && node scripts/fix-cjs-imports.js && node -e \"require('fs').writeFileSync('dist/cjs/package.json', JSON.stringify({type: 'commonjs'}, null, 2))\"",
    "copy-types": "node -e \"const fs = require('fs'); const path = require('path'); fs.readdirSync('dist/esm').filter(f => f.endsWith('.d.ts')).forEach(f => fs.copyFileSync(path.join('dist/esm', f), path.join('dist', f)))\"",
    "clean": "rimraf dist openai-types node_modules",
    "clean:build": "rimraf dist openai-types",
    "dev": "tsc --watch",
    "extract-types": "node extract-types.mjs",
    "fix": "eslint --fix --quiet . ; prettier --write --log-level=silent .",
    "format": "prettier --check \"**/*.{js,ts,tsx}\"",
    "lint": "eslint src",
    "prebuild": "pnpm run clean:build && pnpm run extract-types",
    "prepare": "pnpm run build",
    "prepublishOnly": "pnpm run test",
    "pretest": "pnpm run build",
    "release": "np",
    "test": "pnpm run lint && pnpm run typecheck",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "ky": "^1.7.4"
  },
  "devDependencies": {
    "@dexaai/eslint-config": "^1.3.6",
    "eslint": "^8.57.0",
    "np": "^10.1.0",
    "openai": "^4.77.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.2"
  }
}
