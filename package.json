{
  "name": "openai-fetch-commonjs",
  "version": "3.4.3",
  "description": "OpenAI client powered by fetch (compatible with CommonJS)",
  "repository": "adnahmed/openai-fetch",
  "license": "MIT",
  "author": {
    "name": "Adnan Ahmed Khan",
    "email": "khanadnanahmed01@gmail.com",
    "url": "http://github.com/adnahmed"
  },
  "packageManager": "pnpm@9.12.1",
  "engines": {
    "node": ">=18"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "openai-types"
  ],
  "scripts": {
  "build": "pnpm run build:cjs",
  "build:cjs": "tsc --project tsconfig.cjs.json",
    "clean": "rimraf dist openai-types node_modules",
    "clean:build": "rimraf dist openai-types",
    "dev": "tsc --watch",
    "extract-types": "node extract-types.mjs",
    "fix": "eslint --fix --quiet . ; prettier --write --log-level=silent .",
    "format": "prettier --check \"**/*.{js,ts,tsx}\"",
    "lint": "eslint src",
    "prebuild": "pnpm run clean:build && pnpm run extract-types",
    "prepare": "pnpm run build",
    "prepublishOnly": "pnpm run test",
    "pretest": "pnpm run build",
    "release": "np",
    "test": "pnpm run lint && pnpm run typecheck",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@toss/ky": "^1.2.1"
  },
  "devDependencies": {
    "@dexaai/eslint-config": "^1.3.6",
    "@types/node": "^24.3.1",
    "eslint": "^8.57.0",
    "np": "^10.1.0",
    "openai": "^4.77.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.2"
  }
}
