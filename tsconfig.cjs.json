{
    "extends": "./tsconfig.json",
    "compilerOptions": {
        "module": "CommonJS",
        "moduleResolution": "node",
        "target": "ES2022",
        "sourceMap": true,
        "inlineSources": true,
        "rootDir": "./src",
    "outDir": "./dist",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "baseUrl": "./src",
        "paths": {
            "fetch-api.js": [
                "./fetch-api.cjs.ts"
            ]
        }
    },
    "include": [
        "src"
    ]
}